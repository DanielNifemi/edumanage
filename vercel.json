{
  "version": 2,
  "buildCommand": "sh ./build_files.sh",
  "outputDirectory": "staticfiles_build",
  "builds": [
    {
      "src": "edumanage/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "edumanage/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "edumanage.settings",
    "PYTHONPATH": ".",
    "DEBUG": "False"
  }
}